schema {
    query: Query
}

type Query {
   searchShoes(queryText: String,category: Int,startIndex:Int, offset:Int): [Shoes]
  complexSearch(queryText: String, category: Int,  startPrice: Float,  endPrice: Float, color: String, brand: String,startIndex:Int, offset:Int): [Shoes]

}

type Shoes {
   id: String
   price: Price
   pictureLink:String
   name:String
}
type Price {
   value: Float
   currency: String
}